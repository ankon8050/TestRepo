<h1>Listing mark_sheets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Subject name</th>
      <th>Total number</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mark_sheets.each do |mark_sheet| %>
      <tr>
        <td><%= mark_sheet.name %></td>
        <td><%= mark_sheet.subject_name %></td>
        <td><%= mark_sheet.total_number %></td>
        <td><%= mark_sheet.student_id %></td>
        <td><%= link_to 'Show', mark_sheet %></td>
        <td><%= link_to 'Edit', edit_mark_sheet_path(mark_sheet) %></td>
        <td><%= link_to 'Destroy', mark_sheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mark sheet', new_mark_sheet_path %>
